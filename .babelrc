{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "env": {
      "production": {
          "plugins": ["@babel/plugin-proposal-object-rest-spread", "styled-jsx/babel", "@babel/plugin-proposal-class-properties"]
      },
      "development": {
          "plugins": ["@babel/plugin-proposal-object-rest-spread", "styled-jsx/babel", "@babel/plugin-proposal-class-properties"]
      },
      "test": {
          "plugins": ["@babel/plugin-proposal-object-rest-spread", "styled-jsx/babel-test", "@babel/plugin-proposal-class-properties"]
      }
  }
}
